                                    <input type="radio" class="custom-control-input" name="Answers[@i].StudentAnswer" id="MCQ+@q.QuestionId" value="@choices2[1].ChoiceTxt"disabled>
                                    <span class="custom-control-label">@choices2[1].ChoiceTxt</span>
                                </label>
                                <label class="custom-control custom-radio-md ">
                                    <input type="radio" class="custom-control-input" name="Answers[@i].StudentAnswer" id="MCQ+@q.QuestionId" value="@choices2[2].ChoiceTxt"disabled>
                                    <span class="custom-control-label">@choices2[2].ChoiceTxt</span>
                                </label>
                                    <label class="custom-control custom-radio-md bg-danger ">
                                        <input type="radio" class="custom-control-input" name="Answers[@i].StudentAnswer" id="MCQ+@q.QuestionId" value="@choices2[3].ChoiceTxt"disabled checked>
                                        <span class="custom-control-label">@choices2[3].ChoiceTxt</span>
                                    </label>
                            }
                            else
                            {
                                    <label class="custom-control custom-radio-md ">
                                        <input type="radio" class="custom-control-input" name="Answers[@i].StudentAnswer" id="MCQ+@q.QuestionId" value="@choices2[0].ChoiceTxt"disabled>
                                        <span class="custom-control-label">@choices2[0].ChoiceTxt</span>
                                    </label>
                                    <label class="custom-control custom-radio-md ">
                                        <input type="radio" class="custom-control-input" name="Answers[@i].StudentAnswer" id="MCQ+@q.QuestionId" value="@choices2[1].ChoiceTxt"disabled>
                                        <span class="custom-control-label">@choices2[1].ChoiceTxt</span>
                                    </label>
                                <label class="custom-control custom-radio-md bg-success">
                                    <input type="radio" class="custom-control-input" name="Answers[@i].StudentAnswer" id="MCQ+@q.QuestionId" value="@choices2[2].ChoiceTxt"disabled>
                                    <span class="custom-control-label">@choices2[2].ChoiceTxt</span>
                                </label>
                                    <label class="custom-control custom-radio-md bg-danger ">
                                        <input type="radio" class="custom-control-input" name="Answers[@i].StudentAnswer" id="MCQ+@q.QuestionId" value="@choices2[3].ChoiceTxt"disabled checked>
                                        <span class="custom-control-label">@choices2[3].ChoiceTxt</span>
                                    </label>
                            }
                        }


                    </div>
                </div>
            </div>
        </div>
    }

    counter++;
}
