
@{
    ViewData["Title"] = "Report 05";
    Layout = "~/Views/Shared/_AdminBoardLayout.cshtml";
}

<div id="content" class="p-4 p-md-5 pt-5">
    <div class="content-wrapper">
        <!-- Content Header (Page header) ------>
        <section class="content-header">
            <div class="container-fluid">
                <div class="row mb-2">
                    <div class="col-sm-6">
                        <ol class="breadcrumb float-sm-right">
                            <li class="breadcrumb-item"><a href="#" class="text-secondary">Home</a></li>
                            <li class="breadcrumb-item active">Report 05</li>
                        </ol>
                    </div>
                </div>
            </div><!-- /.container-fluid -->
        </section>

      

        <!-- Main content -->
        <section class="content mb-3">

            <!-- Default box -->
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Report 05</h3>
                </div>
                <div class="card-body p-0">
                    <form asp-action="GetExamByQuestion" asp-controller="Reports" method="post">

                        <label for="Id"> enter exam id </label> 
                        <input name="Id" class="form-control"><br><br>

                    <input type="submit" value="show report 5" class="btn btn-success">
                        <br><br>

                </form>
                </div>
                <!-- /.card-body -->
            </div>
            <!-- /.card -->
        </section>
        <div class="row">
            <div class="col-12">
                <a asp-action="GetExamByQuestion" asp-controller="Reports" class="btn btn-success float-right">Back To List</a>
            </div>
        </div>
        <!-- /.content -->
    </div>
</div>


